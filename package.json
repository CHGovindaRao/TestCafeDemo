{
  "name": "testautomation",
  "version": "1.0.0",
  "description": "Sample test cafe Autometion tests",
  "main": "index.js",
  "dependencies": {
    "testcafe-reporter-html": "^1.4.6"
  },
  "devDependencies": {
    "testcafe": "^1.8.8"
  },
  "scripts": {
    "test": "testcafe chrome sampleTest.js",
    "report": "testcafe chrome sampleTest.js --reporter html:sampleTest.html",
    "headless": "testcafe \"chrome:headless\" sampleTest.js --reporter html:sampleTest.html",
    "screenshot": "testcafe chrome sampleTest.js -S -s screenshots",
    "crossbrowser": "testcafe chrome,firefox sampeTest.js",
    "sameinstance": "testcafe -c 2 chrome sampleTest.js"
  },
